<?xml version="1.0" encoding="UTF-8"?>
<!--
vi:ft=xml
-->
<kml xmlns="http://earth.google.com/kml/2.2">
  <Document>
    <name>{{#name}}{{name}}{{/name}}{{^name}}Shooting Clubs/Ranges{{/name}}</name>
    <Style id="yellow-pushpin-11">
      <IconStyle>
        <scale>1.1</scale>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
        </Icon>
        <hotSpot x="20" y="2" xunits="pixels" yunits="pixels" />
      </IconStyle>
    </Style>
    <Style id="yellow-pushpin-13">
      <IconStyle>
        <scale>1.3</scale>
        <Icon>
          <href>http://maps.google.com/mapfiles/kml/pushpin/ylw-pushpin.png</href>
        </Icon>
        <hotSpot x="20" y="2" xunits="pixels" yunits="pixels" />
      </IconStyle>
    </Style>
    <StyleMap id="yellow-pushpin">
      <Pair>
        <key>normal</key>
        <styleUrl>#yellow-pushpin-11</styleUrl>
      </Pair>
      <Pair>
        <key>highlight</key>
        <styleUrl>#yellow-pushpin-13</styleUrl>
      </Pair>
    </StyleMap>

    {{#name}}
    {{> shootingclub_kml }}
    {{/name}}
    {{^name}}
    <Folder>
      <name>All</name>
      <open>1</open>
      {{#clubs}}
        {{> shootingclub_kml }}
      {{/clubs}}
    </Folder>
    {{/name}}
  </Document>
</kml>

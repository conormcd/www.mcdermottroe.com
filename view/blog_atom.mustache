<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
    <title type="text">{{title}}</title>
    <updated>{{dateISO8601}}</updated>
    <id>{{atomLink}}</id>
    <link rel="self" type="application/atom+xml" href="{{atomLink}}" />
    <link rel="alternate" type="text/html" href="{{link}}" />
    {{#entries}}
    <entry>
        <author>
            <name>Conor McDermottroe</name>
            <uri>http://www.mcdermottroe.com/</uri>
        </author>
        <title type="html"><![CDATA[{{title}}]]></title>
        <updated>{{dateISO8601}}</updated>
        <published>{{dateISO8601}}</published>
        <id>{{link}}</id>
        <link rel="alternate" type="text/html" href="{{link}}" />
        <content type="html"><![CDATA[{{{body}}}]]></content>
    </entry>
    {{/entries}}
</feed>

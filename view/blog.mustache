{{> head}}
{{#entries}}
<div class="blogentry">
<h2><a href="{{link}}">{{title}}</a></h2>
<div class="date">{{date}}</div>
<div class="blogbody">
{{{body}}}
</div>
</div>
{{/entries}}
<div>
    {{#previous}}
    <a href="{{previous}}" style="float: left">&laquo; Newer</a>
    {{/previous}}
    &nbsp;
    {{#next}}
    <a href="{{next}}" style="float: right">Older &raquo;</a>
    {{/next}}
</div>
{{> tail}}

<div class="instagramphoto" itemscope itemtype="http://schema.org/Photograph">
<h2><a href="{{link}}" itemprop="datePublished" content="{{dateISO8601}}">{{title}}</a></h2>
<a href="{{link}}" itemprop="url" content="{{link}}"><img src="{{image}}" height="480" width="480" alt="{{caption}}" itemprop="image" content="{{image}}" /></a>
<div class="caption" itemprop="description">{{caption}}</div>
</div>
